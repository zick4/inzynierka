{
    "name": "xxx/inzynierka",
    "description": "Praca Inżynierska",
    "keywords": ["inzynierka"],
    "license": "commercial",
    "authors": [
        {
            "name": "Paweł Grzeszczak",
            "email": "pgrzeszczak.mail@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-mysql": "*",
        "ext-gmagick": "*",
        "zend/zendframework": "*",
        "doctrine/doctrine1": "*"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-0": {"Org": "library"}
    },
    "repositories": [
        {
            "type": "pear",
            "url": "pear.php.net",
            "vendor-alias": "pear"
        },
        {
            "type": "package",
            "package": {
                "name": "zend/zendframework",
                "version" : "1.12.3",
                "source": {
                    "url": "http://framework.zend.com/svn/framework/standard",
                    "type": "svn",
                    "reference": "tags/release-1.12.3/"
                },
                "autoload": {
                    "psr-0": {
                        "Zend": "library",
                        "ZendX" : "extras/library"
                    }
                },
                "config": {
                    "bin-dir": "bin"
                }
            }
        }
    ]
}
